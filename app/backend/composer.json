{
    "name": "tradity/backend",
    "description": "Tradity Trading Platform Backend",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "cboden/ratchet": "^0.4",
        "firebase/php-jwt": "^6.0",
        "guzzlehttp/guzzle": "^7.5",
        "phpmailer/phpmailer": "^7.0",
        "ratchet/pawl": "0.4.1",
        "react/event-loop": "^1.3",
        "react/promise": "^2.9",
        "react/socket": "^1.12",
        "react/stream": "^1.2",
        "symfony/console": "^5.4",
        "symfony/yaml": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Tradity\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
